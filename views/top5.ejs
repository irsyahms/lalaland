<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <a href="/">Back</a>
    <table border="1">
      <thead>
        <tr>
          <th>Total Vote</th>
          <th>Politician</th>
          <th>Voters</th>
        </tr>
      </thead>
      <tbody>
        <% if(data && data.length > 0){
              data.forEach(d => {
        %>
          <tr>
            <td><%= d.total %></td>
            <td><%= d.name %></td>
            <td>
              <ol>
                <% if(d.voters.length > 0) {
                    d.voters.forEach((vote,index) => {
                %>
                  <li><%= vote %></li>
                <% })} %>
              </ol>
            </td>
          </tr>
        <% })} %>
      </tbody>
    </table>
  </body>
</html>
