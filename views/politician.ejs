<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <table border="1">
      <thead>
        <tr>
          <th>id</th>
          <th>name</th>
          <th>party</th>
          <th>location</th>
          <th>grade_1996</th>
          <th>grade_current</th>
          <th>years_in_congress</th>
          <th>dw1_score</th>
        </tr>
      </thead>
      <tbody>
        <% if(politicians && politicians.length > 0){
              politicians.forEach(politician => {
        %>
          <tr>
            <td><%= politician.id %></td>
            <td><%= politician.name %></td>
            <td><%= politician.party %></td>
            <td><%= politician.location %></td>
            <td><%= politician.grade_1996 %></td>
            <td><%= politician.grade_current %></td>
            <td><%= politician.years_in_congress %></td>
            <td><%= politician.dw1_score %></td>
          </tr>
        <% })} %>
      </tbody>
    </table>
  </body>
</html>
